(require lispy.macros *)
(require leetcode *)

(from leetcode *)

(def solution [n meetings]
  (.sort meetings)
  (= occupied [])
  (= vacancies (list (range n)))
  (heapify vacancies)
  (= cnt (Counter))
  (for m meetings
    (= [s e] m)
    (while (and occupied (<= (sub occupied 0 0) s))
      (heappush vacancies (-> occupied (heappop) (sub 1))))
    (if vacancies
        (do
          (= r (heappop vacancies))
          (heappush occupied [e r]))
        (do
          (= [f r] (heappop occupied))
          (heappush occupied [(+ e (- f s)) r])))
    (++ (sub cnt r)))
  (return (-> cnt (.most-common 1) (sub 0 0))))

(leetcode mostBooked
